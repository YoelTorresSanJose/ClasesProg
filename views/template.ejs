        <button>Ver cursos</button>
        <br>
        
        <table>
            <tr>
                <td>Nombre</td>
                <td>Apellido</td>
            </tr>
            <% for (var i=0; i < users.length; i++) { %>
                <tr>
                    <td>
                        <%= users[i].nombre %>
                    </td>
                    <td>
                        <%= users[i].apellido %>
                    </td>
                </tr>
                <% } %>
        </table>
